            bindings = <&kp MINUS>;
            key-positions = <4 7>;
        };

        underscore {
            bindings = <&kp LS(MINUS)>;
            key-positions = <28 31>;
        };

        equals {
            bindings = <&kp EQUAL>;
            key-positions = <17 18>;
        };

        sleep {
            bindings = <&kp LA(LG(C_SLEEP))>;
            key-positions = <24 35>;
        };

        CapsWord {
            bindings = <&caps_word>;
            key-positions = <5 6>;
    combos {
